<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.sample.EntityTovarEntity">
        <table name="tovar" schema="public" catalog="postgres"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="nameTovar">
                <column name="name_tovar" nullable="false" length="30"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
